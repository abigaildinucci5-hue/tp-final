{
  "expo": {
    "name": "Hotel Luna Serena",
    "slug": "hotel-luna-serena",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/imagenes/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "hotellunaserena",
    "splash": {
      "image": "./src/assets/imagenes/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3A8A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hotellunserena.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/imagenes/icon.png",
        "backgroundColor": "#1E3A8A"
      },
      "package": "com.hotellunserena.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./src/assets/imagenes/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceso a tu galería para que puedas elegir fotos.",
          "cameraPermission": "La app necesita acceso a la cámara para que puedas tomar fotos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir acceso a la ubicación para mostrarte hoteles cercanos."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/imagenes/icon.png",
          "color": "#1E3A8A"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "tu-project-id-aqui"
      }
    }
  }
}